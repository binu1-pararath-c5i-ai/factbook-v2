			    IF (Operation_Cal_Day[YearMonthDay_Sort] <> MONTH([CD]), 1,
			        IF (Operation_Cal_Day[YearMonthDay_Sort] = MONTH([CD]) &&
			            WEEKNUM(Operation_Cal_Day[DayDate], 2) = WEEKNUM([CD], 2), 1, 0
			        )
			    )
