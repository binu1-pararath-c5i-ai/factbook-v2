table Formatted
	lineageTag: 2d77b301-dbcc-4d20-a576-b93d5b52e779

	calculationGroup
		precedence: 2

		calculationItem NONE = SELECTEDMEASURE()

		calculationItem ARROW_NEG =
				
				VAR VarPercent = SELECTEDMEASURE()
				VAR FormattedPercent = IF(
					VarPercent < 0,
					"(" & FORMAT(ABS(VarPercent), "0%") & ")",
					FORMAT(VarPercent, "0%")
				)
				VAR Arrow = IF(VarPercent > 0, UNICHAR(9650), IF(VarPercent < 0, UNICHAR(9660)))
				RETURN FormattedPercent & " " & Arrow

		calculationItem ARROW_POS =
				
				VAR VarPercent = SELECTEDMEASURE()
				VAR Arrow = IF(VarPercent > 0, UNICHAR(9650), IF(VarPercent < 0, UNICHAR(9660)))
				VAR FormattedPercent = IF(VarPercent < 0, FORMAT(ABS(VarPercent), "0%"), FORMAT(VarPercent, "0%"))
				RETURN FormattedPercent & " " & Arrow

		calculationItem ARROW_POS_00 =
				
				VAR VarPercent = SELECTEDMEASURE()
				VAR FormattedPercent = IF(
					VarPercent < 0,
					"(" & FORMAT(ABS(VarPercent), "0.00%") & ")",
					FORMAT(VarPercent, "0.00%")
				)
				VAR Arrow = IF(VarPercent < 0, " " & UNICHAR(9660))
				RETURN FormattedPercent & Arrow

		calculationItem TBL_ARROW =
				
				VAR VarPercent = SELECTEDMEASURE()
				VAR FormattedPercent = IF(
					VarPercent < 0,
					"(" & FORMAT(ABS(VarPercent), "0%") & ")",
					FORMAT(VarPercent, "0%")
				)
				VAR Arrow = IF(VarPercent < 0, " " & UNICHAR(9660))
				RETURN FormattedPercent & Arrow

